type Ant @model {
  id: ID! @isUnique
  name: String!
  color: String!
  length: Int!
  weight: Int!
  image: String!
  calculation: Int!
  calculating: Boolean!
}

type File @model {
  contentType: String!
  createdAt: DateTime!
  id: ID! @isUnique
  name: String!
  secret: String! @isUnique
  size: Int!
  updatedAt: DateTime!
  url: String! @isUnique
}

type User @model {
  createdAt: DateTime!
  id: ID! @isUnique
  updatedAt: DateTime!
}